{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000"
      ],
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "ready - started server on",
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "performance": [
          "error",
          {
            "minScore": 0.8
          }
        ],
        "accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "best-practices": [
          "error",
          {
            "minScore": 0.8
          }
        ],
        "seo": [
          "error",
          {
            "minScore": 0.8
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
